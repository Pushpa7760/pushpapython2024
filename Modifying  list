# append
number=[1,3,6,"God",3.5]
number.append("pushpa")
number.append(10)
print(number)


#insert
number.insert(2,4)
print(number)

# remove 
number.remove("God")
print(number)



#pop
number.pop(1)
print(number)



#len 
x= [2.0,4,67,"hen","cat",8]
len(x)







#sort
y=["Apple","Bat","apple","bat"]
sorted(y)
x=[3,5,8,90,-20]
sorted(x)




